<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<listener>
		<listener-class>com.ontologycentral.edgarwrap.webapp.Listener</listener-class>
	</listener>
	<servlet>
		<servlet-name>FeedServlet</servlet-name>
		<servlet-class>com.ontologycentral.edgarwrap.webapp.FeedServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CikServlet</servlet-name>
		<servlet-class>com.ontologycentral.edgarwrap.webapp.CikServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SicServlet</servlet-name>
		<servlet-class>com.ontologycentral.edgarwrap.webapp.SicServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>TickerServlet</servlet-name>
		<servlet-class>com.ontologycentral.edgarwrap.webapp.TickerServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LinksServlet</servlet-name>
		<servlet-class>com.ontologycentral.edgarwrap.webapp.LinksServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ArchiveServlet</servlet-name>
		<servlet-class>com.ontologycentral.edgarwrap.webapp.ArchiveServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FileServlet</servlet-name>
		<servlet-class>com.ontologycentral.edgarwrap.webapp.FileServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>FeedServlet</servlet-name>
		<url-pattern>/feed/usgaap</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LinksServlet</servlet-name>
		<url-pattern>/cik/links.rdf</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CikServlet</servlet-name>
		<url-pattern>/cik/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SicServlet</servlet-name>
		<url-pattern>/sic/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>TickerServlet</servlet-name>
		<url-pattern>/ticker/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ArchiveServlet</servlet-name>
		<url-pattern>/archive/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FileServlet</servlet-name>
		<url-pattern>/vocab/*</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
	 <mime-mapping>
        <extension>rdf</extension>
        <mime-type>application/rdf+xml</mime-type>
    </mime-mapping>
</web-app>
